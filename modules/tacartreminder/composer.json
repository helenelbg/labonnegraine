{
    "name": "timactive/smartcartreminder",
    "description": "Smart Cart Reminder module for Prestashop",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "TimActive"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "prestashop/php-dev-tools": "^4.3",
        "phpstan/phpstan": "^1.10.15"
    },
    "autoload": {
        "psr-4": {
            "TimActive\\Module\\SmartCartReminder\\": "src/"
        },
        "classmap": [
            "tacartreminder.php",
            "controllers",
            "models"
        ],
        "exclude-from-classmap": []
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "prepend-autoloader": false
    },
    "type": "prestashop-module",
    "author": "PrestaShop"
}